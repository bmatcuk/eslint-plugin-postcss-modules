{
  "name": "eslint-plugin-postcss-modules",
  "version": "1.2.1",
  "description": "Checks that you are using the classes exported by your css modules using postcss.",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "postcss",
    "css-modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bmatcuk/eslint-plugin-postcss-modules"
  },
  "author": "Bob Matcuk <bmatcuk@gmail.com> (https://squeg.net)",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "prepare": "run-s clean build",
    "test": "jest --coverage",
    "watch": "tsc -w -p tsconfig.build.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{j,t}s?(x)": "eslint --fix"
  },
  "peerDependencies": {
    "eslint": ">=0.8.0"
  },
  "devDependencies": {
    "@types/anymatch": "~1.3.1",
    "@types/deasync": "~0.1.0",
    "@types/eslint": "~7.2.0",
    "@types/icss-utils": "~4.1.0",
    "@types/jest": "~26.0.0",
    "@types/node": "~14.0.13",
    "@types/postcss-load-config": "~2.0.0",
    "@types/postcss-modules-extract-imports": "~2.0.0",
    "@types/postcss-modules-local-by-default": "~1.2.0",
    "@types/postcss-modules-scope": "~1.1.0",
    "@types/postcss-modules-values": "~3.0.0",
    "@typescript-eslint/eslint-plugin": "~3.7.0",
    "@typescript-eslint/parser": "~3.7.0",
    "eslint": "~7.5.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-jest": "~23.18.0",
    "eslint-plugin-prettier": "~3.1.0",
    "eslint_d": "~9.1.1",
    "husky": "~4.2.5",
    "jest": "~26.1.0",
    "jest-junit": "~11.0.0",
    "lint-staged": "~10.2.11",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.0.5",
    "rimraf": "~3.0.2",
    "ts-jest": "~26.1.0",
    "typescript": "~3.9.5"
  },
  "dependencies": {
    "anymatch": "^3.0.0",
    "camelcase": "^6.0.0",
    "deasync": "^0.1.0",
    "icss-utils": "^4.1.0",
    "postcss": "^7.0.0",
    "postcss-load-config": "^2.1.0",
    "postcss-modules-extract-imports": "~2.0.0",
    "postcss-modules-local-by-default": "^3.0.2",
    "postcss-modules-scope": "^2.1.0",
    "postcss-modules-values": "^3.0.0"
  }
}
